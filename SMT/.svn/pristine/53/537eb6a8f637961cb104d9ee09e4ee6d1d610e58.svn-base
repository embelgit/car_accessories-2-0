package com.smt.hibernate;

import java.io.Serializable;
import java.util.Date;

public class Visitor implements Serializable {
	
	  
	
	
	  /**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private Long pk_visit_id; 
	  
	  private Long contact;
	  
	  private String name;
	  
	  private String address;
	  
	  private String city;
	  
	  private String landmark;
	  
	  private String occu;
	  
	  private String comname;
	  
	  private String narration;
	
	  private Date reminder;
	
	  
	  public Visitor (Long pk_visit_id,Long contact,String name,String address,String city,String landmark,String occu,String comname,String narration,Date reminder){
		  
		this.setPk_visit_id(pk_visit_id); 
		this.setContact(contact);
		this.setName(comname);
		this.setAddress(address);
		this.setCity(city);
		this.setLandmark(landmark);
		this.setOccu(occu);
		this.setComname(comname);
        this.setNarration(narration);		  
        this.setReminder(reminder);		  
		  
	  }
	  
	  public Visitor(){
		  
	  }


	public Long getContact() {
		return contact;
	}


	public void setContact(Long contact) {
		this.contact = contact;
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public String getAddress() {
		return address;
	}


	public void setAddress(String address) {
		this.address = address;
	}


	public String getCity() {
		return city;
	}


	public void setCity(String city) {
		this.city = city;
	}


	public String getLandmark() {
		return landmark;
	}


	public void setLandmark(String landmark) {
		this.landmark = landmark;
	}


	public String getOccu() {
		return occu;
	}


	public void setOccu(String occu) {
		this.occu = occu;
	}


	public String getComname() {
		return comname;
	}


	public void setComname(String comname) {
		this.comname = comname;
	}


	public String getNarration() {
		return narration;
	}


	public void setNarration(String narration) {
		this.narration = narration;
	}


	public Date getReminder() {
		return reminder;
	}


	public void setReminder(Date reminder) {
		this.reminder = reminder;
	}

	public Long getPk_visit_id() {
		return pk_visit_id;
	}

	public void setPk_visit_id(Long pk_visit_id) {
		this.pk_visit_id = pk_visit_id;
	}
	
	
	
	

}
