<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">

<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<% boolean isHome=false;%>
<%@include file="commons/header.jsp"%>

	<div class="container">
		<h2 class="form-name">Goods Entry </h2>
	</div>
	
	<script src="/SMT/staticContent/js/jquery-1.12.3.min.js"></script>
    <script src="/SMT/staticContent/css/adi.css"></script>
    <link href="/SMT/jsp/goods_entry.jsp" rel="stylesheet"/>
	
	<div id="date">
		<label id="demo"></label>
		<script>
		   var date = new Date();
		   document.getElementById("demo").innerHTML = date.toDateString();;
		</script>
	</div>

<form action="goodsEntry" method="post" class="form-horizontal">
	<div class="form-group-1">
		<div class="col-lg-12">	
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>Challan No:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="value" name="challanNo" class="form-control" placeholder="Challan No" />
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>Invoice No:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="value" name="invoiceNo" class="form-control" placeholder="Invoice No" />
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">	
				<label>Supplier Name:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="text" name="supplierName" class="form-control" placeholder="Supplier Name" />
			</div>
		</div>	
	</div>	
			
	<div class="form-group-1">
		<div class="col-lg-12">	
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">	
				<label>Unloading Location:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="text" name="unloadingLocation" class="form-control" placeholder="Unloading Location" />
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>Transporter Name:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="text" name="transporterName" class="form-control" placeholder="Transporter Name" />
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">	
				<label>PO Check By:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="text" name="poCheckBy" class="form-control" placeholder="PO Check By" />
			</div>			
		</div>	
	</div>
			
	<div class="form-group-1">
		<div class="col-lg-12">	
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>Remarks:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="text" name="remarks" class="form-control" placeholder="Remarks" />
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">	
				<label>Purchase Entry By:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="text" name="purchaseEntryBy" class="form-control" placeholder="Purchase Entry By" />
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>Sale Price By:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="text" name="salePriceBy" class="form-control" placeholder="Sale Price By" />
			</div>
		</div>	
	</div>	
				
	<div class="form-group-1">
		<div class="col-lg-12">	
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>DN No:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="value" name="dnNo" class="form-control" placeholder="DN No"  />
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>Debit Amount:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="value" name="debitAmount" class="form-control" placeholder="Debit Amount" />
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>CN No:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="value" name="cnNo" class="form-control" placeholder="CN No"  />
			</div>
		</div>	
	</div>	
			
	<div class="form-group-1">
		<div class="col-lg-12">	
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>Credit Amount:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="value" name="creditAmount" class="form-control" placeholder="Credit Amount"  />
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>Remarks:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="text" name="remarks" class="form-control" placeholder="Remarks" />
			</div>
		</div>	
	</div>
			
	
	<!--------------------------------------ACCOUNT SECTION--------------------------------------------------->	
	<div align="center">
	    <h2>ACCOUNT</h2>
	</div>
			
	<div class="form-group-1">
		<div class="col-lg-12">	
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>Bill Check By:</label>
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="text" name="Bill check by" class="form-control" placeholder="Bill Check By" />
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>Date:</label>
			</div>
		
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="date" name="date" class="form-control" />
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">	
				<label>Tally entry check by:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="text" name="Tally entry check by" class="form-control" placeholder="Tally entry check by" />
			</div>
		</div>	
	</div>	
				
	<div class="form-group-1">
		<div class="col-lg-12">	
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>Payment Authorised By:</label>
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="text" name="Payment authorised by" class="form-control" placeholder="Payment Authorised By" />
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>Payment Mode:</label>
			</div>
		
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<select name="Payment mode" class="form-control">
					<option value="selected">--Payment Mode--</option>
					<option value="cheque">Cheque</option>
					<option value="neft">NEFT</option>
					<option value="rtgs">RTGS</option>
					<option value="cash">Cash</option>
					<option value="other">Other</option>
				</select>
			</div>
			
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">	
				<label for="Payment Details">Payment Details:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="value" name="Payment Details" class="form-control" placeholder="Payment Details" />
			</div>
		</div>	
	</div>	
				
	<div class="form-group-1">
		<div class="col-lg-12">
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<label>Date:</label>
			</div>
			<div class="col-lg-2 col-md-2 col-sm-2 col-xs-6">
				<input type="date" name="date" class="form-control" />
			</div>
		</div>
	</div>	
			
	<div>
		<h4 align="center">FOR INTERNAL CIRCULATION ONLY. IF FOUND,KINDLY RETURN TO ACCOUNT DEPARTMENT</h4>
	</div>
	

	<div class="wrapper">
		<input type="button" value="Submit" class="btn btn-lg btn-success btn-md" />
	</div>
	
</form>

	<%@include file="commons/footer.jsp"%>