package com.smt.hibernate;

import java.io.Serializable;

public class Broker implements Serializable{
	
	
	private Long pk_broker_id;

	private String firstName;
	 
	private String middleName; 
	
	private String lastName;
	
	private String accType;
	
	private Long contactNumber;
	
	private String address;
	
	private Long accNumber;
	
	public Broker (Long pk_broker_id, String firstName, String middleName, String lastName, String accType, Long contactNumber, String address, Long accNumber){
		
		this.setPk_broker_id(pk_broker_id);
		this.setFirstName(firstName);
		this.setMiddleName(middleName);
		this.setLastName(lastName);
		this.setAccType(accType);
		this.setContactNumber(contactNumber);
		this.setAddress(address);
		this.setAccNumber(accNumber);
		
	}
	
	public Broker(){
		
	}

	public Long getPk_broker_id() {
		return pk_broker_id;
	}

	public void setPk_broker_id(Long pk_broker_id) {
		this.pk_broker_id = pk_broker_id;
	}

	public String getFirstName() {
		return firstName;
	}

	public void setFirstName(String firstName) {
		this.firstName = firstName;
	}

	public String getMiddleName() {
		return middleName;
	}

	public void setMiddleName(String middleName) {
		this.middleName = middleName;
	}

	public String getLastName() {
		return lastName;
	}

	public void setLastName(String lastName) {
		this.lastName = lastName;
	}

	public String getAccType() {
		return accType;
	}

	public void setAccType(String accType) {
		this.accType = accType;
	}

	public Long getContactNumber() {
		return contactNumber;
	}

	public void setContactNumber(Long contactNumber) {
		this.contactNumber = contactNumber;
	}

	public String getAddress() {
		return address;
	}

	public void setAddress(String address) {
		this.address = address;
	}

	public Long getAccNumber() {
		return accNumber;
	}

	public void setAccNumber(Long accNumber) {
		this.accNumber = accNumber;
	}
	
	

}
