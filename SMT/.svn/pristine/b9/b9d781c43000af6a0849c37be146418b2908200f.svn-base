<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.2

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.smt.hibernate.CustCredit" 
    table="customer_credit"
>

    <id
        name="pk_cust_Id"
        type="java.lang.Long"
        column="pk_cust_Id"
    >
    
        <generator class="increment" />
    </id>
    
     <property
        name="billDate"
        type="java.sql.Date"
        column="Date"
        not-null="true"
        
    />
    
     <property
        name="billNo"
        type="java.lang.Long"
        column="bill_no"
        not-null="true"
        length="15"
    />
    
    <property
        name="name"
        type="java.lang.String"
        column="f_name"
        not-null="true"
        length="15"
    />
    
    <property
        name="middleName"
        type="java.lang.String"
        column="m_name"
        not-null="true"
        length="15"
    />
    
    <property
        name="lastName"
        type="java.lang.String"
        column="l_name"
        not-null="true"
        length="15"
    />
    
     <property
        name="address"
        type="java.lang.String"
        column="address"
        not-null="true"
        length="15"
    />
    
     <property
        name="landmark"
        type="java.lang.String"
        column="landmark"
        not-null="true"
        length="15"
    />
    
    <property
        name="state"
        type="java.lang.String"
        column="state"
        not-null="true"
        length="15"
    />
    
     <property
        name="district"
        type="java.lang.String"
        column="district"
        not-null="true"
        length="15"
    />
    
    <property
        name="city"
        type="java.lang.String"
        column="city"
        not-null="true"
        length="15"
    />
    
     <property
        name="pinCode"
        type="java.lang.Long"
        column="pin_code"
        not-null="true"
        length="10"
    />
    
    <property
        name="mobileNo"
        type="java.lang.Long"
        column="mobile_no"
        not-null="true"
        length="15"
    />
    
     <property
        name="emailId"
        type="java.lang.String"
        column="email"
        not-null="true"
        length="20"
    />
    
     <property
        name="referedBy"
        type="java.lang.String"
        column="referance_by"
        not-null="true"
        length="20"
    />
    
 </class>
</hibernate-mapping>