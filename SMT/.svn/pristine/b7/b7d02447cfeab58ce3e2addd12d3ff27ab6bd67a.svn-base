<%@include file="commons/header.jsp"%>

	<div class="container">
		<h2 class="form-name">Input VAT</h2>
	</div>
	
	<script src="/SMT/staticContent/js/jquery-1.12.3.min.js"></script>
    <script src="/SMT/staticContent/css/adi.css"></script>
    <link href="/SMT/jsp/input_vat.jsp" rel="stylesheet"/>
    
    <div id="date">
		<label id="demo"></label>
		<script>
		   var date = new Date();
		   document.getElementById("demo").innerHTML = date.toDateString();;
		</script>
	</div>
    
<form action="inputVat" method="post" class="form-horizontal">
	<div class="date-field col-lg-12 col-md-12 col-sm-12 col-xs-12">
		<strong>From:</strong> <input type="date" name="date" />
		<strong>To:</strong>  <input type="date" name="date" /> 
		<input class="btn btn-success" type="submit" value="Search" >
	</div>

	<div class="wrapper">
		<input class="btn btn-lg btn-primary btn-md" type="submit" value="Set Column" /> &nbsp &nbsp
		<input class="btn btn-lg btn-info btn-md" type="reset" value="Print" />
	</div>
	
	<div class="rTable">
		<div class="rTableRow">
			<div class="rTableHead col-md-1"><strong>Date</strong></div>
			<div class="rTableHead col-md-1"><strong>Entry Name</strong></div>
			<div class="rTableHead col-md-1"><strong>Entry Type</strong></div>
			<div class="rTableHead col-md-2"><strong>Party Name</strong></div>
			<div class="rTableHead col-md-2"><strong>Set Registration No</strong></div>
			<div class="rTableHead col-md-1"><strong>NET Total</strong></div>
			<div class="rTableHead col-md-1"><strong>Total VAT</strong></div>
			<div class="rTableHead col-md-1"><strong>Other</strong></div>
			<div class="rTableHead col-md-2"><strong>Gross Total</strong></div>
		</div>
		
		<div class="rTableRow">
			<div class="rTableCell col-md-1"><input type="text" class="form-control"></div>
			<div class="rTableCell col-md-1"><input type="text" class="form-control"></div>
			<div class="rTableCell col-md-1"><input type="text" class="form-control"></div>
			<div class="rTableCell col-md-2"><input type="text" class="form-control"></div>
			<div class="rTableCell col-md-2"><input type="text" class="form-control"></div>
			<div class="rTableCell col-md-1"><input type="text" class="form-control"></div>
			<div class="rTableCell col-md-1"><input type="text" class="form-control"></div>
			<div class="rTableCell col-md-1"><input type="text" class="form-control"></div>
			<div class="rTableCell col-md-2"><input type="text" class="form-control"></div>
		</div>
	</div>
	
</form>
	
