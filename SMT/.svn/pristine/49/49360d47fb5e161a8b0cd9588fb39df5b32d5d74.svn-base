<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.2

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.smt.hibernate.UserDetail" 
    table="user_details"
>

    <id
        name="pkUserId"
        type="java.lang.Long"
        column="pk_user_id"
    >
    
        <generator class="assigned" />
    </id>

    <property
        name="addressLine1"
        type="java.lang.String"
        column="address_line1"
        length="255"
    />
    <property
        name="addressLine2"
        type="java.lang.String"
        column="address_line2"
        length="255"
    />
    <property
        name="firstName"
        type="java.lang.String"
        column="first_name"
        length="255"
    />
    <property
        name="lastName"
        type="java.lang.String"
        column="last_name"
        length="255"
    />
    <property
        name="middleName"
        type="java.lang.String"
        column="middle_name"
        length="255"
    />
    <property
        name="mobileNumber1"
        type="java.lang.String"
        column="mobile_number1"
        length="255"
    />
    <property
        name="mobileNumber2"
        type="java.lang.String"
        column="mobile_number2"
        length="255"
    />

    <!-- Associations -->
  

</class>
</hibernate-mapping>
